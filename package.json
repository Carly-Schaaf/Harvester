{
  "name": "mern",
  "version": "1.0.0",
  "description": "mern stack project",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "frontend-sass": "npm run sass --prefix frontend",
    "frontend-build": "npm run build --prefix frontend",
    "dev": "concurrently \"npm run server:debug\" \"npm run frontend\" \"npm run frontend-sass\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Carly-Schaaf/mern.git"
  },
  "keywords": [
    "mern"
  ],
  "author": "carly schaaf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Carly-Schaaf/mern/issues"
  },
  "homepage": "https://github.com/Carly-Schaaf/mern#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "express": "^4.17.0",
    "express-graphql": "^0.9.0",
    "faker": "^4.1.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "jquery": "^3.4.1",
    "jsdom": "^15.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.6.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-spinners": "^0.5.13",
    "validator": "^10.11.0",
    "webpack": "^4.39.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
